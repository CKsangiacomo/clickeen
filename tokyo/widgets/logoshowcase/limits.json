      "sanitizeTo": "",
      "enforce": { "load": "sanitize", "ops": "reject", "publish": "reject" }
    },
    {
      "kind": "flag",
      "key": "links.enabled",
      "paths": ["strips[].logos[].targetBlank", "strips[].logos[].nofollow"],
