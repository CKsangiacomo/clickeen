      "deny": false,
      "sanitizeTo": true,
      "enforce": { "load": "sanitize", "ops": "reject", "publish": "reject" }
    },
    {
      "kind": "flag",
      "key": "links.enabled",
      "path": "strips[].logos[].href",
      "mode": "nonempty-string",
