#!/usr/bin/env python3
import re

def refactor_preview_specs(html_content):
    """
    Refactor .preview-specs blocks to wrap title+detail pairs in .preview-specs__row divs
    """
    count = 0

    # Pattern to match a preview-specs block with its contents
    # We need to handle both single-line and multi-line blocks
    def replace_specs_block(match):
        nonlocal count
        opening = match.group(1)  # Everything before the content
        content = match.group(2)  # The content between > and </div>

        # Check if this block already has .preview-specs__row divs
        if 'preview-specs__row' in content:
            return match.group(0)  # Already refactored, skip

        # Find all title+detail pairs
        # Pattern: <span class="preview-specs__title">...</span><span class="preview-specs__detail">...</span>
        pair_pattern = r'(<span class="preview-specs__title">.*?</span>)\s*(<span class="preview-specs__detail">.*?</span>)'

        # Check if this content has title/detail pairs
        if not re.search(pair_pattern, content, re.DOTALL):
            return match.group(0)  # No pairs to wrap, skip

        # Wrap each pair in a .preview-specs__row div
        def wrap_pair(pair_match):
            title = pair_match.group(1)
            detail = pair_match.group(2)
            return f'<div class="preview-specs__row">\n      {title}\n      {detail}\n    </div>'

        new_content = re.sub(pair_pattern, wrap_pair, content, flags=re.DOTALL)
        count += 1

        return f'{opening}>{new_content}</div>'

    # Match preview-specs blocks
    # Pattern: <div class="...preview-specs...">content</div>
    pattern = r'(<div class="[^"]*preview-specs[^"]*"[^>]*)(>)(.*?)(</div>)'

    result = re.sub(pattern, replace_specs_block, html_content, flags=re.DOTALL)

    return result, count

# Read the file
with open('/Users/piero_macpro/code/VS/clickeen/dieter/dieteradmin/src/html/dieter-showcase/colors.html', 'r') as f:
    content = f.read()

# Refactor
new_content, blocks_updated = refactor_preview_specs(content)

# Write back
with open('/Users/piero_macpro/code/VS/clickeen/dieter/dieteradmin/src/html/dieter-showcase/colors.html', 'w') as f:
    f.write(new_content)

print(f"Refactoring complete! Updated {blocks_updated} .preview-specs blocks")
