{
  "extends": "../tsconfig.app-base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@paris/*": [
        "./*"
      ],
      "@paris/lib/*": [
        "lib/*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
